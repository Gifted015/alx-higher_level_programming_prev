#!/usr/bin/python3
def delete_at(my_list=[], idx=0):
    if idx < 0 or idx > len(my_list) - 1:
        return my_list
    else:
        a = []
        
        for x in range(len(my_list)):
            if x == idx:
                continue
            else:
                a.append(my_list[x])
        my_list = 's'
        return my_list
